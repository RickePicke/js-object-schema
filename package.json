  ],
  "devDependencies": {
    "@babel/core": "7.11.6",
    "@babel/polyfill": "7.11.5",
    "@babel/preset-env": "7.11.5",
    "@rollup/plugin-babel": "5.2.1",
    "babel-jest": "26.3.0",
    "eslint": "7.9.0",
    "eslint-config-standard": "14.1.1",
    "eslint-plugin-import": "2.22.0",
